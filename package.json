{
  "name": "simple-query",
  "version": "2.0.0-beta4",
  "description": "Lightweight implementation of jQuery api subset",
  "repository": {
    "type": "git",
    "url": "https://github.com/dbrekalo/simplequery"
  },
  "main": "src/core.js",
  "license": "MIT",
  "scripts": {
    "watch": "grunt build && grunt watch",
    "build": "grunt build",
    "watch:test": "node ./node_modules/karma/bin/karma start --browsers PhantomJS",
    "test": "node ./node_modules/karma/bin/karma start --single-run --browsers PhantomJS",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "bump:patch": "grunt bump:patch",
    "bump:minor": "grunt bump:minor",
    "bump:major": "grunt bump:major"
  },
  "keywords": [
    "jQuery",
    "SimpleQuery",
    "simple-query"
  ],
  "devDependencies": {
    "attire": "^1.3.2",
    "chai": "^4.0.2",
    "coveralls": "^2.11.15",
    "grunt": "^1.0.1",
    "grunt-bump": "^0.8.0",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-eslint": "^20.0.0",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jquery": "^3.2.1",
    "karma": "^1.4.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.31",
    "karma-webpack": "^2.0.2",
    "load-grunt-tasks": "^3.5.2",
    "mocha": "^3.2.0",
    "mocha-loader": "^1.1.0",
    "pretender": "^1.4.2",
    "webpack": "^2.2.1"
  }
}
